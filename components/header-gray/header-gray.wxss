/* components/header-gray/header-gray.wxss */
page{
  position: absolute;
  top:0;
  left:0;
  right:0;
  bottom:0;
  background:#eee;
}
.header{
    position:fixed;
    top:0;
    width:100%;
    height: 110rpx;
    z-index:3;
    background:#f9fafd;
}
.header-content{
    position: absolute;
    left: 120rpx;
    right: 120rpx;
    font-size:32rpx;
    font-weight: bold;
    text-align: center;
    line-height:110rpx;
}