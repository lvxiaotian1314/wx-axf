/* pages/cart/cart.wxss */
.wraper{
  position: absolute;
  top:110rpx;
  width:100%;
  height:96px;
  background:#fff;
}
view{
  box-sizing: border-box;
}
/*用户地址信息*/
.userAddress{
  position: relative;
  background-position: left top,left bottom;
  background-repeat:repeat-x;
  background-size:auto 6rpx;
  background-image: url(data:image/png;base64,),url(data:image/png;base64,) 
}
.userAddress>view{
  height: 32px;
  padding-left:40rpx;
  font-size:32rpx;
  line-height: 32px;
}
.position{
  visibility: hidden;
}
.userInfo{
  margin-left:40rpx;
}
view.repair{
  position: absolute;
  height: 96px;
  right:80rpx;
  top:0;
  line-height: 96px;
}
view.repair::after{
  content: "";
  display: inline-block;
  width:40rpx;
  height:96px;
  vertical-align: top;
  background:url('data:image/png;base64,') 10rpx center no-repeat;
  background-size:60rpx;
}